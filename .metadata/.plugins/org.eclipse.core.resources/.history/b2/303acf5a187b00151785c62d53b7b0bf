package financeDataService;

import java.rmi.RemoteException;

public interface FinanceFormDataService {
	public boolean init(String operator) throws RemoteException;//期初建账建立新账户
	
	public boolean save(String operator) throws RemoteException;//期初建账备份
	
	public boolean create(String time1, String time2) throws RemoteException;//导出报表
	
	public void finish() throws RemoteException;
}
