package ELMS.businesslogic.UserBL;

public class MockUserVO {
	
	

	String id=" ";
	String name=" ";
	String password=" ";
	String job=" ";
	
	public MockUserVO(String id,String n,String p,String job){
		this.id=id;
		this.name=n;
		this.password=p;
		this.job=job;
	}	
	
	public MockUserVO() {

	}

	public String getId() {
		return id;
	}

	public String getName() {
		return name;
	}

	public String getPassword() {
		return password;
	}

	public String getJob() {
		return job;
	}

}
